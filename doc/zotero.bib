	file = {PDF:C\:\\Users\\Soham\\Zotero\\storage\\6DX9A9QY\\Paine et al. - 2023 - Quantum Quantile Mechanics Solving Stochastic Differential Equations for Generating Time-Series.pdf:application/pdf},
