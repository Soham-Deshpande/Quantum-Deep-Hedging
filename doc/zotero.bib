	pages = {64},
	file = {PDF:C\:\\Users\\Soham\\Zotero\\storage\\EYXPBPLP\\Dudas et al. - 2023 - Quantum reservoir computing implementation on coherently coupled quantum oscillators.pdf:application/pdf},
