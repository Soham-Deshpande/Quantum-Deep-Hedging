	pages = {57},
	file = {PDF:C\:\\Users\\Soham\\Zotero\\storage\\FA7TJPK8\\Sornsaeng et al. - 2024 - Quantum next generation reservoir computing an efficient quantum algorithm for forecasting quantum.pdf:application/pdf},
}

